{
  "name": "vite_electron",
  "private": true,
  "version": "0.0.1",
  "main": "main.js",
  "scripts": {
    "start": "vite",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test_el": "electron .",
    "dev": "concurrently \"(cross-env BROWSER=none vite)\" \"(wait-on http://127.0.0.1:3000 && electron .)\"",
    "build": "tsc && vite build",
    "build:electron": "electron-builder  --mac "
  },
  "dependencies": {
  
    "electron-updater": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0-beta.0",
    "concurrently": "^8.0.1",
    "electron-log": "^5.0.0-beta.23",
    "cross-env": "^7.0.3",
    "electron": "^24.1.2",
    "electron-builder": "^23.6.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "typescript": "^5.0.2",
    "vite": "^4.3.0",
    "wait-on": "^7.0.1"
  },
  "build": {
    "appId": "com.example.app",
    "productName": "my app",
    "copyright": "",
    "publish": [
      {
        "provider": "generic",
        "url": "https://gitee.com/gepengbo/electron_update"
      }
    ],
    "directories": {
      "buildResources": "dist",
      "output": "build"
    },
    "nsis": {
      "oneClick": false,
      "language": "2052",
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "files": [
      "dist/**/*",
      "./main.js"
    ],
    "extends": null
  }
}
